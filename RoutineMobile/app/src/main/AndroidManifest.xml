<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wizard.routinemobile" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat" >
        <activity
            android:name="com.wizard.routinemobile.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wizard.routinemobile.RoutineActivity"
            android:label="@string/title_activity_routine1" >
        </activity>
        <activity
            android:name="com.wizard.routinemobile.SettingActivity"
            android:label="@string/title_activity_setting" >
        </activity>
    </application>

</manifest>